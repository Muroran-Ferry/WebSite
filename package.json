{
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": "~20.11",
    "pnpm": ">=8.15.5",
    "preinstall": "npx only-allow pnpm"
  },
  "scripts": {
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && pnpm lint:style && pnpm lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm lint:js --fix && pnpm lint:style --fix",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "husky install",
    "postinstall": "nuxt prepare"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh hooks/pre-commit"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@fancyapps/ui": "5.0.35",
    "@googlemaps/js-api-loader": "1.16.6",
    "@headlessui/vue": "1.7.22",
    "@heroicons/vue": "2.1.3",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/web-vitals": "0.2.6",
    "@tailwindcss/typography": "0.5.10",
    "@types/google.maps": "3.55.5",
    "@types/node": "20.11.30",
    "@yeger/vue-masonry-wall": "5.0.8",
    "autoprefixer": "10.4.18",
    "cdate": "0.0.7",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-tailwindcss": "3.15.1",
    "eslint-plugin-vue": "9.25.0",
    "husky": "9.0.11",
    "nuxt": "3.11.1",
    "nuxt-jsonld": "2.0.8",
    "postcss": "8.4.37",
    "postcss-html": "1.6.0",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.12",
    "rollup-plugin-webpack-stats": "0.2.4",
    "scroll-hint": "1.2.5",
    "stylelint": "16.3.1",
    "stylelint-config-standard": "36.0.0",
    "tailwindcss": "3.4.1",
    "volar-service-vetur": "0.0.34",
    "vue-gtag-next": "1.14.0",
    "zod": "3.22.4"
  }
}
